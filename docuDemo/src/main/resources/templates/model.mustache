<!doctype html>
<html>
  <head>
    <title>{{{appName}}}</title>
     <style type="text/css">
      {{>style.css}}
    </style>
    <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
     <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css"  rel="stylesheet">
  <!-- Latest compiled and minified JavaScript -->
 <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
  <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  </head>
  <body>
  <h1>{{{appName}}}</h1>
     {{> header}}
     <div class="container-fluid">
     <div class="row">
      <div class="col-md-4">
     <h4> <a href="index.html">Index Page</a></h4>
      </div>
  <div class="col-md-4" style="padding-top: 82px;">
  {{#modelDetailsList}}
  <div class="modelItem">
  <h3>Title : <a name="_{{modelTitle}}">{{modelTitle}}</a></h3>
   <table class="table table-hover">
    <thead>
      <tr>
        <th>Element</th>
        <th>Meaning</th>
        <th>Type</th>
         <th>Reference</th>
      </tr>
      </thead>
   <tbody>
  {{#modelList}}
 
  <tr>
  <td>{{modelName}}</td>
  <td>{{{modelDesc}}}</td>
  <td>{{modelType}}</td>
  <td><a href="modelnew.html#_{{modelRef}}">{{modelRef}}</a>
  {{^modelRef}}
  -
  {{/modelRef}}
  </td> 
  </tr>
  
  {{/modelList}}
  </tbody>
  <br>
  </table>
  </div>
  {{/modelDetailsList}}
  </div> <!--col-md-4 -->
  <div class="col-md-4>
  </div>
  </div> <!--row ends -->
  </div> <!--container-fluid ends -->
  
  </body>
  
  
  </html>


